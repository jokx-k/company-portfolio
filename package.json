{
  "name": "oud-reserve-portfolio",
  "version": "1.0.0",
  "description": "Luxury real estate company portfolio website for Oud Reserve",
  "main": "index.html",
  "scripts": {
    "start": "live-server --port=8080 --host=localhost --entry-file=index.html",
    "dev": "live-server --port=3000 --host=localhost --entry-file=index.html --watch=css,js",
    "build": "npm run minify-css && npm run minify-js",
    "minify-css": "cleancss -o css/styles.min.css css/styles.css",
    "minify-js": "uglifyjs js/main.js -o js/main.min.js",
    "optimize-images": "imagemin images/**/* --out-dir=images/optimized",
    "lint-css": "stylelint css/*.css",
    "lint-js": "eslint js/*.js"
  },
  "keywords": [
    "real-estate",
    "luxury",
    "portfolio",
    "arabic",
    "rtl",
    "responsive",
    "oud-reserve"
  ],
  "author": "Link Expert",
  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.2",
    "clean-css-cli": "^5.6.2",
    "uglify-js": "^3.17.4",
    "imagemin": "^8.0.1",
    "imagemin-jpeg-recompress": "^7.0.0",
    "imagemin-pngquant": "^9.0.2",
    "stylelint": "^15.10.3",
    "eslint": "^8.47.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/oud-reserve-portfolio.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/oud-reserve-portfolio/issues"
  },
  "homepage": "https://your-username.github.io/oud-reserve-portfolio"
}
